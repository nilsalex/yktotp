{
  "name": "yktotp",
  "version": "0.1.0",
  "description": "Get a YubiKey OTP into the browser",
  "repository": "git@github.com:nilsalex/yktotp.git",
  "author": "Nils Alex <nils@famalex.de>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist/",
    "copy-files": "mkdir -p dist && cp src/manifest.json src/popup.html dist/",
    "build": "yarn run clean && tsc && yarn run copy-files",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write ."
  },
  "devDependencies": {
    "@types/chrome": "^0.0.181",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  }
}
